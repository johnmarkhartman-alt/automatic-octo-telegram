   "outputs": [],
   "source": [
    "repeat_offenders <- b_scored %>%\n",
    "  group_by(supplier_id, supplier_name, product_category, region) %>%\n",
    "  summarise(\n",
    "    total_findings = n(),\n",
    "    repeat_findings = sum(repeat_finding),\n",
    "    repeat_rate = repeat_findings / total_findings,\n",
    "    major_critical = sum(severity %in% c(\"Major\", \"Critical\")),\n",
    "    major_critical_repeat = sum(repeat_finding == 1 & severity %in% c(\"Major\", \"Critical\")),\n",
    "    avg_capa_days = mean(days_to_close_capa),\n",
    "    total_points = sum(weighted_risk_points),\n",
    "    .groups = \"drop\"\n",
    "  ) %>%\n",
    "  filter(total_findings >= 8) %>%\n",
    "  arrange(desc(major_critical_repeat), desc(repeat_rate), desc(total_points))\n"
   ]
  },
