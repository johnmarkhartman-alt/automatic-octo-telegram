      "$ weighted_risk_points \u001b[3m\u001b[90m<dbl>\u001b[39m\u001b[23m 1.75, 1.00, 3.00, 1.00, 8.75, 5.00, 1.00, 5.25, 1…\n"
     ]
    }
   ],
   "source": [
    "# notebook setup\n",
    "library(tidyverse)\n",
    "library(lubridate)\n",
    "library(viridis)\n",
    "\n",
    "find_file <- function(filename) {\n",
    "  if (file.exists(filename)) return(filename)\n",
    "  candidates <- list.files(\"/kaggle/input\", recursive = TRUE, full.names = TRUE)\n",
    "  hit <- candidates[str_detect(candidates, paste0(filename, \"$\"))]\n",
    "  if (length(hit) == 0) stop(paste(\"Could not find\", filename, \"- upload it or add it as a Kaggle Dataset.\"))\n",
    "  hit[1]\n",
    "}\n",
    "\n",
    "master_path   <- find_file(\"supplier_master_vietnam_v2.csv\")\n",
    "findings_path <- find_file(\"supplier_audit_findings.csv\")\n",
    "\n",
    "suppliers <- readr::read_csv(master_path, show_col_types = FALSE)\n",
    "findings  <- readr::read_csv(findings_path, show_col_types = FALSE) %>%\n",
    "  mutate(audit_date = as.Date(audit_date))\n",
    "\n",
    "severity_weights <- tibble(\n",
    "  severity = c(\"Minor\", \"Major\", \"Critical\"),\n",
    "  severity_weight = c(1, 3, 5)\n",
    ")\n",
    "\n",
    "repeat_multiplier <- 1.75\n",
    "\n",
    "b_scored <- findings %>%\n",
    "  left_join(\n",
    "    suppliers %>% select(supplier_id, supplier_name, product_category, region, certifications),\n",
    "    by = \"supplier_id\"\n",
    "  ) %>%\n",
    "  left_join(severity_weights, by = \"severity\") %>%\n",
    "  mutate(\n",
    "    repeat_mult = if_else(repeat_finding == 1, repeat_multiplier, 1),\n",
    "    weighted_risk_points = severity_weight * repeat_mult\n",
    "  )\n",
    "\n",
    "glimpse(b_scored)\n"
   ]
  },
