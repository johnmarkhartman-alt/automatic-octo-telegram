   "source": [
    "supplier_risk_load %>%\n",
    "  summarise(\n",
    "    n_suppliers = n(),\n",
    "    suppliers_for_70pct = min(rank[cum_pct_risk >= 0.70]),\n",
    "    suppliers_for_80pct = min(rank[cum_pct_risk >= 0.80]),\n",
    "    pct_suppliers_for_70pct = suppliers_for_70pct / n_suppliers,\n",
    "    pct_suppliers_for_80pct = suppliers_for_80pct / n_suppliers\n",
    "  )\n"
   ]
