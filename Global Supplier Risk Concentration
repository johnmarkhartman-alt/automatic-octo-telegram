   "source": [
    "supplier_risk_load <- b_scored %>%\n",
    "  group_by(supplier_id, supplier_name, product_category, region) %>%\n",
    "  summarise(\n",
    "    total_points = sum(weighted_risk_points),\n",
    "    findings = n(),\n",
    "    repeats = sum(repeat_finding),\n",
    "    avg_capa_days = mean(days_to_close_capa),\n",
    "    .groups = \"drop\"\n",
    "  ) %>%\n",
    "  arrange(desc(total_points)) %>%\n",
    "  mutate(\n",
    "    rank = row_number(),\n",
    "    pct_total_risk = total_points / sum(total_points),\n",
    "    cum_pct_risk = cumsum(pct_total_risk),\n",
    "    pct_suppliers = rank / n()\n",
    "  )\n",
    "# pareto\n",
    "ggplot(supplier_risk_load, aes(x = rank)) +\n",
    "  geom_col(aes(y = total_points), alpha = 0.85) +\n",
    "  geom_line(aes(y = cum_pct_risk * max(total_points)), linewidth = 1) +\n",
    "  scale_y_continuous(\n",
    "    name = \"Weighted Risk Points (bars)\",\n",
    "    sec.axis = sec_axis(~ . / max(supplier_risk_load$total_points),\n",
    "                        name = \"Cumulative Share of Total Risk (line)\",\n",
    "                        labels = scales::percent_format(accuracy = 1))\n",
    "  ) +\n",
    "  labs(\n",
    "    title = \"Pareto of Supplier Food Safety Risk Load\",\n",
    "    x = \"Supplier Rank (Highest Risk Load to Lowest)\",\n",
    "    y = NULL\n",
    "  ) +\n",
    "  theme_minimal()\n"
   ]
  },
