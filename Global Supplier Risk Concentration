     "output_type": "display_data"
    }
   ],
   "source": [
    "priority_list <- supplier_risk_load %>%\n",
    "  select(supplier_id, supplier_name, product_category, region, total_points, findings, repeats, avg_capa_days, rank) %>%\n",
    "  left_join(\n",
    "    repeat_offenders %>% select(supplier_id, repeat_rate, major_critical_repeat),\n",
    "    by = \"supplier_id\"\n",
    "  ) %>%\n",
    "  mutate(\n",
    "    priority_flag = case_when(\n",
    "      rank <= 15 ~ \"Tier 1: Immediate Intervention\",\n",
    "      rank <= 35 ~ \"Tier 2: Monitor + Targeted Controls\",\n",
    "      TRUE ~ \"Tier 3: Routine Oversight\"\n",
    "    )\n",
    "  ) %>%\n",
    "  arrange(rank)\n",
    "\n",
    "priority_list %>% head(25)\n"
   ]
  },
