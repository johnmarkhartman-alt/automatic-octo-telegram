   "source": [
    "data_overview <- b_scored %>%\n",
    "  summarise(\n",
    "    rows = n(),\n",
    "    suppliers = n_distinct(supplier_id),\n",
    "    date_min = min(audit_date),\n",
    "    date_max = max(audit_date),\n",
    "    missing_capa_days = sum(is.na(days_to_close_capa))\n",
    "  )\n",
    "\n",
    "data_overview\n"
   ]
