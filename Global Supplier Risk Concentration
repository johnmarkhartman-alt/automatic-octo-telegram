   "source": [
    "summary_stats <- b_scored %>%\n",
    "  summarise(\n",
    "    findings = n(),\n",
    "    suppliers = n_distinct(supplier_id),\n",
    "    repeat_rate = mean(repeat_finding),\n",
    "    avg_capa_days = mean(days_to_close_capa),\n",
    "    pct_major_critical = mean(severity %in% c(\"Major\",\"Critical\"))\n",
    "  )\n",
    "\n",
    "summary_stats\n"
   ]
